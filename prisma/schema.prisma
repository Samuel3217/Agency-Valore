generator client {
  provider = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model product {
  id         Int      @id @default(autoincrement())
  name       String
  description String?
  price      Float
  picture    String?
  createAt   DateTime @default(now())
  updatedAt  DateTime @updatedAt
}

model nuevos {
  id         Int      @id @default(autoincrement())
  name       String
  description String?
  price      Float
  picture    String?
  createAt   DateTime @default(now())
  updatedAt  DateTime @updatedAt
}
